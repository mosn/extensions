{
  "name": "egress_sofa_bolt",
  "type": "egress",
  "address": "0.0.0.0:12220",
  "bind_port": true,
  "access_logs": [
    {
      "log_path": "./logs/mosn/egress.log"
    }
  ],
  "filter_chains": [
    {
      "filters": [
        {
          "type": "tcpcopy",
          "config": {
            "port": "12220"
          }
        },
        {
          "type": "proxy",
          "config": {
            "name": "proxy_config",
            "downstream_protocol": "X",
            "upstream_protocol": "X",
            "extend_config": {
              "sub_protocol": "bolt"
            },
            "router_config_name": "sofa_egress_bolt_router"
          }
        },
        {
          "type": "connection_manager",
          "config": {
            "router_config_name": "sofa_egress_bolt_router",
            "router_configs": "./mosn/conf/routers/sofa_egress_bolt_router/"
          }
        }
      ]
    }
  ],
  "stream_filters": [
    {
      "type": "govern_config"
    },
    {
      "type": "transcoder"
    },
    {
      "type": "tamperproofing"
    },
    {
      "type": "rpc_service_tracer"
    },
    {
      "type": "cloud_metrics"
    },
    {
      "type": "fault_inject",
      "config": {
        "direction": "outbound"
      }
    },
    {
      "type": "downgrade"
    },
    {
      "type": "alipayLimitFilter",
      "config": {
        "enable": true,
        "direction": "outbound"
      }
    },
    {
      "type": "identity-provider"
    },
    {
      "type": "fault_tolerance_send"
    },
    {
      "type": "fault_tolerance_choose"
    },
    {
      "type": "alipayResourceAopFilter",
      "config": {
        "direction": "outbound"
      }
    },
    {
      "type": "egress_shm_check"
    }

  ]
}
