{
  "name": "ingress_sp",
  "type": "ingress",
  "address": "0.0.0.0:13080",
  "bind_port": true,
  "access_logs": [
    {
      "log_path": "./logs/mosn/egress.log"
    }
  ],
  "filter_chains": [
    {
      "tls_context_set": [
        {}
      ],
      "filters": [
        {
          "type": "proxy",
          "config": {
            "downstream_protocol": "sp",
            "name": "proxy_config",
            "router_config_name": "ingress_sp_router",
            "upstream_protocol": "sp"
          }
        },
        {
          "type": "connection_manager",
          "config": {
            "router_config_name": "ingress_sp_router",
            "router_configs": "./mosn/conf/routers/ingress_sp_router/"
          }
        }
      ]
    }
  ],
  "stream_filters": [
    {
      "type": "stream_strategy_filter"
    },
    {
      "type": "transcoder"
    },
    {
      "type": "tamperproofing"
    },
    {
      "type": "rpc_service_tracer"
    },
    {
      "type": "cloud_metrics"
    },
    {
      "type": "fault_inject",
      "config": {
        "direction": "outbound"
      }
    },
    {
      "type": "downgrade"
    },
    {
      "type": "alipayLimitFilter",
      "config": {
        "enable": true,
        "direction": "outbound"
      }
    },
    {
      "type": "fault_tolerance_send"
    },
    {
      "type": "fault_tolerance_choose"
    }
  ]
}
